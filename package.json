{
  "name": "notes-app",
  "private": true,
  "version": "1.0.0",
  "description": "",
  "workspaces": [
    "api",
    "app"
  ],
  "engines": {
    "npm": "7.9.0"
  },
  "main": "index.js",
  "scripts": {
    "build": "SKIP_PREFLIGHT_CHECK=true npm run build --workspace=app",
    "lint": "eslint ./app ./api",
    "start": "cross-env npm run start --workspace=api",
    "dev:api": "npm run build && npm run dev --workspace=api"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "eslint-plugin-cypress": "2.12.1",
    "standard": "16.0.3",
    "cross-env": "7.0.3"
  },
  "eslintConfig": {
    "ignorePatterns": "build",
    "env": {
      "jest": true,
      "cypress/globals": true
    },
    "extends": "./node_modules/standard/eslintrc.json",
    "rules": {
      "no-unused-vars": "warn"
    },
    "plugins": [
      "cypress"
    ]
  }
}
